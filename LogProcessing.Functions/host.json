{
    "version": "2.0",
  "logging": {
    "logLevel": {
      "Host.Triggers.DurableTask": "Warning",
      "Microsoft": "Warning",
      "Azure": "Warning",
      "DurableTask":  "Warning" 
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Dependency;Exception;Event",
